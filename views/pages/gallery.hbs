<h1 class ="gallery-title">Gallery</h1>







<div class="section">
{{#each gallery}}
    <div class="each-photo"> 
      <img style="width:20rem" src="{{imageUrl}}" alt="{{title}}"/> 
    <hr>
    <h4>{{title}}</h4>
    <h6>By: {{author}}<h6>
    <img class="stars-image" src="/images/{{average}}-starRate.png"/>    
    <a class ="link-button" href="/gallery/{{_id}}">See details</a> <a class ="link-button" href="/gallery/{{_id}}/edit">Edit</a>
    </div>
<br><br>
{{/each}}
<br><br>
</div>
